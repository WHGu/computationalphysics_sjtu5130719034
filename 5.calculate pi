#include<math.h>
#include<stdio.h>

int main(void)
{
    int n = 20 ;
    double f = 0;
    double s = 0;
    double e = 0;
    double pi = 3.141592653589793;

    for(int i = n; i>0 ;i--)
    {
        s = 4*pow(-1,i-1)/(2*i-1);
        f = f+s;
        e = (f-pi)/pi;
    }
     printf(" value = %f\n error = %g \n n = %i",f,e,n);
}
